var beginnerIosDevelopment: [String] =
    
                        [
                                "Swift Basic",
                                "Collections and Control Flow",
                                "Functions in Swift",
                                "Object Oriented Swift",
                                "Build simple iPhone app in Swift",
                                "Enumerations & Optionals",
                                "Protocols",
                                "Error Handling",
                                "Build vending machine appl",
                                "Introduction to auto layout in IOS"
                        ]

beginnerIosDevelopment[0] = "Swift Basics" // Modify Array Elements
beginnerIosDevelopment[1] = "Collections and Control Flows" // Modify Array Elements

var completedTasks: [String] = ["Completed Tasks"] // Create new array 

for i in 0..<2 // Create for loop for to go through [0,1] array elements 

{
    completedTasks += [beginnerIosDevelopment[i]] // Add array element to completedTasks array
    
}

for _ in 0..<2 {
beginnerIosDevelopment.remove(at: 0) // Remove array elements from beginnerIosDevelopment
}

print(completedTasks) // Print array to console
